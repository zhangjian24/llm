#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/jianzhang/Documents/Codes/llm/node_modules/.pnpm/playwright-core@1.58.2/node_modules/playwright-core/node_modules:/mnt/d/jianzhang/Documents/Codes/llm/node_modules/.pnpm/playwright-core@1.58.2/node_modules:/mnt/d/jianzhang/Documents/Codes/llm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/jianzhang/Documents/Codes/llm/node_modules/.pnpm/playwright-core@1.58.2/node_modules/playwright-core/node_modules:/mnt/d/jianzhang/Documents/Codes/llm/node_modules/.pnpm/playwright-core@1.58.2/node_modules:/mnt/d/jianzhang/Documents/Codes/llm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../playwright-core/cli.js" "$@"
else
  exec node  "$basedir/../playwright-core/cli.js" "$@"
fi
